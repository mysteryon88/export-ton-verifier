{
  "name": "export-ton-verifier",
  "version": "2.0.0",
  "description": "Tool for generating Groth16 and PLONK verifier code for the TON blockchain from SnarkJS .zkey files.",
  "homepage": "https://github.com/mysteryon88/export-ton-verifier#readme",
  "bugs": {
    "url": "https://github.com/mysteryon88/export-ton-verifier/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mysteryon88/export-ton-verifier.git"
  },
  "keywords": [
    "zksnark",
    "groth16",
    "plonk",
    "ton",
    "func",
    "tact",
    "tolk",
    "zero",
    "knowledge",
    "circuit"
  ],
  "license": "MIT",
  "author": "white_coon",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "export-ton-verifier": "./dist/cli.js"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npx tsup && npm pack"
  },
  "dependencies": {
    "@ton/core": "~0",
    "ejs": "^4.0.1",
    "ffjavascript": "^0.3.1",
    "snarkjs": "^0.7.6"
  },
  "devDependencies": {
    "tsup": "^8.5.1"
  }
}